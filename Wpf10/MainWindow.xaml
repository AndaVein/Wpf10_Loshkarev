<Window x:Class="Wpf10.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf10"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0">ПР10</TextBlock>
        <StackPanel Grid.Column="1" Grid.RowSpan="2" Grid.Row="0">
            <Button Height="40" Click="Button_Click_1" Margin="74,0,0,0">Вход</Button>
            <Button Height="40" Click="Button_Click" Margin="74,0,0,0">Регистрация</Button>
        </StackPanel>
        <Frame Name="FrmMain" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="2" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
